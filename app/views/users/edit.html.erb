<h1>Edit User</h1>

<%=form_for @user do |f|%>
  <div>
    <%= f.label :email%>
    <%= f.email_field :email%>
  </div>
  <div>
    <%= f.label :full_name%>
    <%= f.text_field :full_name%>
  </div>
  <div>
    <%= f.label :password%>
    <%= f.password_field :password%>
  </div>
  <div>
    <%= f.label :password_confirmation%>
    <%= f.password_field :password_confirmation%>
  </div>
  <div>
    <%= f.submit "signup"%>
  </div>
<%end%>