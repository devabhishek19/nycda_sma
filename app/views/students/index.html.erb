<h1>Students List</h1><br><br>
<div>
  <ul>
  	<%@students.each do |student|%>
  		<li>
  			 <span><%=link_to student.email, student %></span> | 
  			 <span><%=student.fname%></span> | 
  			 <span><%=student.lname%></span>
  		</li>
  	<%end%>
  </ul>
</div><br><br>
<p>
  <%=button_to "Add a new Student", new_student_path, method: :get%>
</p>
